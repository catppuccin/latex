{{ '\preparecolorset{RGB/HTML}{Ctp' }}{{ flavor.identifier | capitalize }}{{ '}{}{%' }}{% for _, color in flavor.colors %}
	{{ color.identifier | capitalize }},{{ color.rgb.r }},{{ color.rgb.g }},{{ color.rgb.b }}/{{ color.hex | upper }}{% if not loop.last %};{% endif %}%{% endfor %}
